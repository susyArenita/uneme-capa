doctype html
html
  head
    title UNEME - CAPA
    
    script(src="javascripts/jquery-3.1.1.js")
    script(type="text/javascript" src="javascripts/materialize.min.js")
    
    link(rel="stylesheet" type="text/css" href="stylesheets/style.css")
    link(rel="stylesheet" type="text/css" href="stylesheets/materialize.min.css")
    link(rel="stylesheet" type="text/css" href="fonts/iconfont/material-icons.css")
      
    meta(name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1")
    
  body
        div(class="z-depth-3 col s12 m10 l10 offset-l1")
          div(class="row")
            br
            div(class="col s2 push-s1")
              img(src="images/logo.jpg" width="200" height="100" class="")
            div(class="col s6 ")
              h4(class="right green-text darken-3") UNEME - CAPA
          div(class="row")
            div(class="col s2 push-s1")
              ul(id="slide-out" class="sidenav")
                li
                  div(class="user-view")
                    div(class="background")
                      img(src="images/fondo.jpg")
                    a(href="#user")
                      img(class="circle" src="images/user_soporte.png")
                    a(href="#name")
                      span(class="black-text name") Psicologo
                    a(href="#email")
                      span(class="black-text email") prueba_psicologo@gmail.com
                li
                  a(href="tratamientoPrincipal") Tratamiento clínico
                    i(class="material-icons") assignment_ind
                li
                  a(href="historial") Historial notas tratamiento
                    i(class="material-icons") assignment 
                li
                  a(href="tratamientoRegistro") Registro nota tratamiento
                    i(class="material-icons") subject
                li
                  a(href="egreso") Egreso
                    i(class="material-icons") assignment_turned_in
                li
                  a(href="historialEgresos") Historial egresos
                    i(class="material-icons") folder_shared
                li
                  a(href="citasP") Agendar citas
                    i(class="material-icons") event_note
                li
                  a(href="/logout/") Cerrar sesión
                    i(class="material-icons") exit_to_app
              a(href="#" data-target="slide-out" class="sidenav-trigger")
                i(class="material-icons" ) menu   
        

        div(class="row padding-top padding-bottom")
          div(class="col s6 push-s3")
            h5(class="green darken-2" class="center white-text") Detalles nota de tratamiento
        div(class="container")
          br
          div(class="row padding-top padding-bottom")
            div(class="z-depth-1 col s12 m10 l10 offset-l1")
              div(class="row")
                form(class="col s10 push-s1 pull-s1")
                  div(class="row")
                    div(class="input-field col s6 pull-6")
                      input(name="NoExpediente" id="NoExpediente" type="text" class="validate" value=data.nota_tratamiento.NumeroExpediente readonly)                   
                      label(for="NoExpediente") Número de expediente
                  div(class="row")
                    div(class="input-field col s12")
                      input(name="Nombre" id="NombrePaciente" type="text" class="validate" value=data.nota_tratamiento.Nombre readonly)
                      label(for="NombrePaciente") Nombre del paciente
                  div(class="row")
                    div(class="input-field col s4 ")
                      input(name="Sesion" id="Sesion" type="text" class="validate" value=data.nota_tratamiento.NumeroSesionNotaTratamiento readonly)
                      label(for="Fecha") No. sesión
                    div(class="input-field col s4 ")
                      input(name="Fecha" id="Fecha" type="text" class="validate" value=data.nota_tratamiento.FechaNota readonly)
                      label(for="Fecha") Fecha
                    div(class="input-field col s4")
                      input(name="Hora" id="Hora" type="text" class="validate" value=data.nota_tratamiento.HoraInicioNotaTratamiento readonly)
                      label(for="Hora") Hora
                  div(class="row")
                    div(class="input-field col s12")
                       textarea(name="Resumen" id="Resumen" class="materialize-textarea" readonly)=data.nota_tratamiento.ResumenNotaTratamiento
                       label(for="Resumen") Resumen de la nota
                  div(class="row")
                    div(class="input-field col s12")
                       textarea(name="Diagnostico" id="Diagnostico" class="materialize-textarea" readonly)=data.nota_tratamiento.DiagnosticoNotaTratamiento
                       label(for="Diagnostico") Diagnóstico
                  div(class="row")
                    div(class="input-field col s12")
                      textarea(name="Pronostico" id="Pronostico" class="materialize-textarea" readonly)=data.nota_tratamiento.PronosticoNotaTratamiento
                      label(for="Pronostico") Pronóstico
                  div(class="row")
                    div(class="col s5 push-s4 ")
                      a(class="waves-effect waves-light btn right green darken-1" target="_blank" href=`/tratamientoPDF/${data.nota_tratamiento.idNotaSeguimiento}`) PDF
                    div(clas="col s5 pull-s3")
                      a(class="waves-effect waves-light btn right blue-grey darken-4 ") Inicio  
    
    
        script.
              (function($){
                $(function(){
                $('.sidenav').sidenav();
                });
              })(jQuery);
          
          
